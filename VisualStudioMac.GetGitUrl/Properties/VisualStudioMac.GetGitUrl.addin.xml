<?xml version="1.0" encoding="UTF-8"?>
<ExtensionModel>
	<Runtime>
		<Import assembly="VisualStudioMac.GetGitUrl.dll" />
	</Runtime>
	<!-- Commands -->
	<Extension path="/MonoDevelop/Ide/Commands">
		<Command id="VisualStudioMac.GetGitUrl.CommandHandlers.GetGitUrlCommandHandler" _label="Copy Git Url for selected line(s).." defaultHandler="VisualStudioMac.GetGitUrl.CommandHandlers.GetGitUrlCommandHandler" />
	</Extension>
	<!-- Editor Context Menu -->
	<Extension path="/MonoDevelop/TextEditor/ContextMenu/Editor">
		<Condition id="FileType" fileExtensions=".cs">
			<CommandItem id="VisualStudioMac.GetGitUrl.CommandHandlers.GetGitUrlCommandHandler" />
		</Condition>
	</Extension>
	<!-- Main Tool Menu Entries -->
	<Extension path="/MonoDevelop/Ide/MainMenu/Tools">
		<CommandItem id="VisualStudioMac.GetGitUrl.CommandHandlers.GetGitUrlCommandHandler" />
	</Extension>
</ExtensionModel>